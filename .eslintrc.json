{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": [
        "google", "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
